        <div class="col-lg-1"></div>
        <form>
        <div class="col-lg-10">
            <div class="card">
                <div class="card-header">
                    <strong>Add Product</strong>
                    <small> Form</small>
                </div>
                <div class="card-body card-block">
                    <div class="form-group">
                        <label for="brand" class=" form-control-label">Brand/Company Name</label>
                        <input type="text" id="brand" placeholder="Enter brand name" class="form-control">
                    </div>
                    <div class="form-group">
                        <label for="name" class=" form-control-label">Product Name</label>
                        <input type="text" id="pname" placeholder="Enter product name" class="form-control">
                    </div>
                    <div class="form-group">
                        <label for="serialno" class=" form-control-label">Serial No.</label>
                        <input type="text" id="serialno" placeholder="Enter Unique Serial No." class="form-control">
                    </div>
                    <hr/>
                    <div class="row form-group">
                        <div class="col-3">
                            <div class="form-group">
                                <label for="buy" class=" form-control-label">Buying Price</label>
                                <input type="text" id="buy" placeholder="Enter buying price" class="form-control">
                            </div>
                        </div>
                        <div class="col-3">
                            <div class="form-group">
                                <label for="sell" class=" form-control-label">Selling Price</label>
                                <input type="text" id="sell" placeholder="Enter selling price" class="form-control">
                            </div>
                        </div>
                        <div class="col-3">
                            <div class="form-group">
                                <label for="CGST" class=" form-control-label">CGST(%)</label>
                                <select class="form-control" id="CGST" required>
                                    <optgroup label="CGST">
                                        <option value="#" selected>Choose CGST</option>
                                        <option value="2.5">2.5%</option>
                                        <option value="6">6%</option>
                                        <option value="9">9%</option>
                                        <option value="14">14%</option>
                                    </optgroup>
                                </select>

                            </div>
                        </div>
                        <div class="col-3">
                            <div class="form-group">
                                <label for="SGST" class=" form-control-label">SGST/UTGST(%)</label>
                                <select class="form-control" id="SGST" required>
                                    <optgroup label="SGST/UTGST">
                                        <option value="#" selected>Choose SGST/UTGST</option>
                                        <option value="2.5">2.5%</option>
                                        <option value="6">6%</option>
                                        <option value="9">9%</option>
                                        <option value="14">14%</option>
                                    </optgroup>
                                </select>
                            </div>
                        </div>
                    </div>
                <div class="row form-group">
                    <div class="col-4"></div>
                    <div class="col-2">                              
                        <div class="form-group text-center"><button type="submit" class="btn btn-outline-success"><i class="fa fa-magic"></i>&nbsp; Submit</button></div>
                    </div>
                    <div class="col-2">
                        <div class="form-group text-center"><button type="reset" class="btn btn-outline-warning"><i class="fa fa-map-marker"></i>&nbsp; Reset</button></div>
                    </div>    
                </div>
            </div>
            </div>
          </div>
        </form>
